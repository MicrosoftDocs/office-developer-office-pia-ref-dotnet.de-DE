    <Member MemberName="Promote">
      <MemberSignature Language="C#" Value="public void Promote ();" />
      <MemberSignature Language="ILAsm" Value=".method public hidebysig newslot virtual instance void Promote() runtime managed" />
      <MemberSignature Language="DocId" Value="M:Microsoft.Office.Core.SmartArtNode.Promote" />
      <MemberSignature Language="VB.NET" Value="Public Sub Promote ()" />
